<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.professor.ProfileFragment">

    <Button
        android:id="@+id/logOutBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginBottom="16dp"
        android:onClick="onLogOutClick"
        android:text="@string/btn_logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/modProfileBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="41dp"
        android:layout_marginBottom="16dp"
        android:onClick="onModProfileClick"
        android:text="Modifica Profilo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginTop="51dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_launcher_background"
        tools:src="@drawable/ic_launcher_background"
        tools:srcCompat="@drawable/ic_launcher_background" />

    <TextView
        android:id="@+id/matricolaText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:text="Matricola Qui"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/corsoText"
        app:layout_constraintTop_toBottomOf="@+id/corsoText" />

    <TextView
        android:id="@+id/usernameText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="17dp"
        android:text="Username Qui"
        android:textAlignment="viewStart"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <TextView
        android:id="@+id/corsoText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Corso Di Laurea Qui"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/usernameText"
        app:layout_constraintTop_toBottomOf="@+id/usernameText" />

    <androidx.core.widget.NestedScrollView
        android:id="@+id/nestedScrollView"
        android:layout_width="267dp"
        android:layout_height="392dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="62dp"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="@+id/logOutBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.25">

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            tools:layout_conversion_absoluteHeight="444dp"
            tools:layout_conversion_absoluteWidth="381dp"
            tools:layout_editor_absoluteX="14dp"
            tools:layout_editor_absoluteY="208dp">

            <TextView
                android:id="@+id/textView17"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Nome:"
                android:textAlignment="viewStart"
                tools:layout_conversion_absoluteHeight="20dp"
                tools:layout_conversion_absoluteWidth="267dp"
                tools:layout_editor_absoluteX="72dp"
                tools:layout_editor_absoluteY="207dp" />

            <EditText
                android:id="@+id/formName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="NOME QUI"
                android:inputType="textPersonName"
                android:textAlignment="viewStart"
                tools:layout_conversion_absoluteHeight="46dp"
                tools:layout_conversion_absoluteWidth="267dp"
                tools:layout_editor_absoluteX="72dp"
                tools:layout_editor_absoluteY="227dp" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Cognome:"
                android:textAlignment="viewStart"
                tools:layout_conversion_absoluteHeight="20dp"
                tools:layout_conversion_absoluteWidth="267dp"
                tools:layout_editor_absoluteX="72dp"
                tools:layout_editor_absoluteY="273dp" />

            <EditText
                android:id="@+id/formSurname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="COGNOME QUI"
                android:inputType="textPersonName"
                android:textAlignment="viewStart"
                tools:layout_conversion_absoluteHeight="46dp"
                tools:layout_conversion_absoluteWidth="267dp"
                tools:layout_editor_absoluteX="72dp"
                tools:layout_editor_absoluteY="293dp" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Data di Nascita:"
                android:textAlignment="viewStart"
                tools:layout_conversion_absoluteHeight="20dp"
                tools:layout_conversion_absoluteWidth="267dp"
                tools:layout_editor_absoluteX="72dp"
                tools:layout_editor_absoluteY="338dp" />

            <EditText
                android:id="@+id/formDateBirth"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="DATA DI NASCITA QUI"
                android:inputType="textPersonName"
                android:textAlignment="viewStart"
                tools:layout_conversion_absoluteHeight="46dp"
                tools:layout_conversion_absoluteWidth="267dp"
                tools:layout_editor_absoluteX="72dp"
                tools:layout_editor_absoluteY="358dp" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Citta:"
                android:textAlignment="viewStart"
                tools:layout_conversion_absoluteHeight="20dp"
                tools:layout_conversion_absoluteWidth="267dp"
                tools:layout_editor_absoluteX="72dp"
                tools:layout_editor_absoluteY="404dp" />

            <EditText
                android:id="@+id/formCity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="CITTA QUI"
                android:inputType="textPersonName"
                android:textAlignment="viewStart"
                tools:layout_conversion_absoluteHeight="46dp"
                tools:layout_conversion_absoluteWidth="267dp"
                tools:layout_editor_absoluteX="72dp"
                tools:layout_editor_absoluteY="424dp" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Residenza:"
                android:textAlignment="viewStart"
                tools:layout_conversion_absoluteHeight="20dp"
                tools:layout_conversion_absoluteWidth="267dp"
                tools:layout_editor_absoluteX="72dp"
                tools:layout_editor_absoluteY="469dp" />

            <EditText
                android:id="@+id/formAddress"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="RESIDENZA QUI"
                android:inputType="textPersonName"
                android:textAlignment="viewStart"
                tools:layout_conversion_absoluteHeight="46dp"
                tools:layout_conversion_absoluteWidth="267dp"
                tools:layout_editor_absoluteX="72dp"
                tools:layout_editor_absoluteY="489dp" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="AltroCazzoQui"
                android:textAlignment="viewStart"
                tools:layout_conversion_absoluteHeight="20dp"
                tools:layout_conversion_absoluteWidth="267dp"
                tools:layout_editor_absoluteX="72dp"
                tools:layout_editor_absoluteY="535dp" />

            <EditText
                android:id="@+id/editTextTextPersonName6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="QWERTY"
                android:inputType="textPersonName"
                android:textAlignment="viewStart"
                tools:layout_conversion_absoluteHeight="46dp"
                tools:layout_conversion_absoluteWidth="267dp"
                tools:layout_editor_absoluteX="72dp"
                tools:layout_editor_absoluteY="555dp" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="AltroCazzoAncoraQui"
                android:textAlignment="viewStart"
                tools:layout_conversion_absoluteHeight="20dp"
                tools:layout_conversion_absoluteWidth="267dp"
                tools:layout_editor_absoluteX="72dp"
                tools:layout_editor_absoluteY="600dp" />

            <EditText
                android:id="@+id/editTextTextPersonName4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:hint="QWERTY2"
                android:inputType="textPersonName"
                android:textAlignment="viewStart"
                tools:layout_conversion_absoluteHeight="46dp"
                tools:layout_conversion_absoluteWidth="267dp"
                tools:layout_editor_absoluteX="72dp"
                tools:layout_editor_absoluteY="620dp" />

        </LinearLayout>
    </androidx.core.widget.NestedScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>